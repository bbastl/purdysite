/*ADDED 2013 */
.wrap { width: 764px; margin: 0 auto; text-align: left; }
#header_container { width:764px; height:250px; background:url(<%= asset_path "headers/2ndhead_products.jpg" %>) no-repeat left bottom;}
#blue_line { margin-bottom:1px; width:764px; height: 7px; 
	background-image:url(<%= asset_path "dots/dot_green.gif" %>); float: left; clear:both;}
#logo { width: 102px; height: 68px;
		background-image:url(<%=  asset_path "logos/logo_purdy.gif" %>);
		float: left; clear:left;
	  }
#navbar_header {
	float: left; clear:right;
	width:662px;height:51px;
}
#navbar_header .links  {
	float: right; clear:both; width:640px; height:40px;
}
#navbar_header .search_form {
	 width:230px; height:25px; float: right; clear:both;
}

ul.header_links { float:right; clear:both; height:25px; margin:4px; margin-right:1px; width:580px;padding: 0px;
					list-style-type: none; margin: 0px;}

li.header_links { display: inline; list-style-type: none;font-size: 12px;
				  height:24px; font-weight: bold;  }
li.header_links a {color: #666; text-decoration: none;}
li.header_links a:hover {color: #018181; text-decoration: none;}

#footer { float:left; clear:both; width:764px;
			font-size: 10px; text-decoration: none; color: #666666}
#footer .copyright {
	background-color: #666666; color:#fff; height:18px; padding-top: 4px; 
	padding-left: 4px;
}

#footer .copyright p {
	height:12px; margin: 0px;
}

#main_content {
	width:764px; margin-top:8px; font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px; color: #333333;
}

#main_content .content {
	float: right; width: 454px;
}

#main_content .left_links ul {
	float: left; width: 200px; list-style-type: none;
}

#main_content .left_links li {
	float: left; width: 200px; margin:3px;
}

#main_content .left_links a:hover {
	color: #777;
}

#catalog {
	float:left; clear:both;
}

